{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "@devcorp-libs/ng-streams-killer": {
      "projectType": "library",
      "root": "libs/ng-streams-killer",
      "sourceRoot": "libs/ng-streams-killer/src",
      "prefix": "dcl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "libs/ng-streams-killer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-streams-killer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ng-streams-killer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/ng-streams-killer/src/test.ts",
            "tsConfig": "libs/ng-streams-killer/tsconfig.spec.json",
            "karmaConfig": "libs/ng-streams-killer/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "libs/ng-streams-killer/**/*.ts",
              "libs/ng-streams-killer/**/*.html"
            ]
          }
        }
      }
    },
    "@devcorp-libs/dialog": {
      "projectType": "library",
      "root": "libs/dialog",
      "sourceRoot": "libs/dialog/src",
      "prefix": "dcl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "libs/dialog/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/dialog/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/dialog/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/dialog/src/test.ts",
            "tsConfig": "libs/dialog/tsconfig.spec.json",
            "karmaConfig": "libs/dialog/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "libs/dialog/**/*.ts",
              "libs/dialog/**/*.html"
            ]
          }
        }
      }
    },
    "@devcorp-libs/store-util": {
      "projectType": "library",
      "root": "libs/store-util",
      "sourceRoot": "libs/store-util/src",
      "prefix": "dcl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "libs/store-util/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/store-util/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/store-util/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/store-util/src/test.ts",
            "tsConfig": "libs/store-util/tsconfig.spec.json",
            "karmaConfig": "libs/store-util/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "libs/store-util/**/*.ts",
              "libs/store-util/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  },
  "defaultProject": "@devcorp-libs/ng-streams-killer"
}
